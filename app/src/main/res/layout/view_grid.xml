        <LinearLayout
            android:layout_width="0dp"
            android:layout_weight="0.33"
            android:background="#ffffff"
            android:layout_marginLeft="2dp"
            android:layout_height="match_parent"></LinearLayout>
        <LinearLayout
            android:layout_width="0dp"
            android:layout_weight="0.33"
            android:layout_marginLeft="2dp"
